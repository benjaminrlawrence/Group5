<!DOCTYPE html>
<html>
<head>
  <title>Group5</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="main-container" class="container">
  <div class="row">
    <div class="col-xs-3">
      <!--@TODO add sidebar-->
      <h3>Site Name or Logo Goes Here</h3>
      <br />

    </div>
    <div class="col-xs-9">
      <div class="col-xs-6"></div>
      <div class="col-xs-6">
        <%= render 'layouts/user_widget' %>
      </div>
      <p class="warning"><%= notice %></p>
      <p class="alert warning"><%= alert %></p>
      <%= yield %>

    </div>
  </div>
</div>

</body>
</html>
