<%= form_for :query, :url=>{:action=>"search"} do |f| %>
    
        General Search: <%= f.text_field(:type_general) %> <br>
		Hashtag Search: <%= f.text_field(:type_hashtag) %> <br>
		User Search:    <%= f.text_field(:type_twitter_user) %> <br>
        String Search:  <%= f.text_field(:search_query) %> <br>
		<%= submit_tag "Search"%>
<% end %>